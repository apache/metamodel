<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean class="org.apache.metamodel.spring.DataContextFactoryBean">
		<property name="type" value="Excel" />
		<property name="filename" value="src/test/resources/data/example.xlsx" />
		<property name="skipEmptyLines" value="true" />
		<property name="skipEmptyColumns" value="true" />
	</bean>

</beans>